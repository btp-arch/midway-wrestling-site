---
const redirectTo = "/schedule";
const title = "Redirecting to Schedule | Midway Wrestling Club";
---
<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="refresh" content={`0; url=${redirectTo}`} />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="canonical" href={redirectTo} />
    <title>{title}</title>
  </head>
  <body>
    <p>
      This page has moved. <a href={redirectTo}>Go to the Schedule page</a>.
    </p>
  </body>
</html>
